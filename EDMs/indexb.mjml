<mjml>
  <mj-head>
    <mj-font name="Raleway" href="https://fonts.googleapis.com/css?family=Raleway:100,300,400,700"></mj-font>
    <mj-style>
      @import url('https://fonts.googleapis.com/css?family=Raleway:400,700');

      .text {
        font-family: Raleway, line-height: 0px
      }
    </mj-style>
  </mj-head>
  <mj-body background-color="#d6dde5">
    <mj-section background-color="#ffa59e" padding-bottom="20px" padding-top="20px">
